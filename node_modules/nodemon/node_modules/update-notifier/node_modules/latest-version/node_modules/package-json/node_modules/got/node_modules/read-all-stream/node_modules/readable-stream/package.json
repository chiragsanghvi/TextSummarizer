{
  "name": "readable-stream",
  "version": "2.2.8",
  "description": "Streams3, a user-land copy of the stream library from Node.js",
  "main": "readable.js",
  "dependencies": {
    "buffer-shims": "~1.0.0",
    "core-util-is": "~1.0.0",
    "isarray": "~1.0.0",
    "inherits": "~2.0.1",
    "process-nextick-args": "~1.0.6",
    "string_decoder": "~1.0.0",
    "util-deprecate": "~1.0.1"
  },
  "devDependencies": {
    "assert": "~1.4.0",
